{
    "version": "2.0.0",
    "tasks": [
      {
        "label": "Run ESLint",
        "command": "npx eslint . --ext .js,.jsx,.ts,.tsx",
        "type": "shell",
        "problemMatcher": ["$eslint-stylish"],
        "group": {
          "kind": "build",
          "isDefault": true
        }
      }
    ]
  }